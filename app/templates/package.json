{
  "name": "<%= name %>-service",
  "version": "0.0.1",
  "author": "<%= author %> <<%= email %>>",
  "private": true,
  "scripts": {
    "start": "node <%= name %>-service.js",
    "debug": "node-debug <%= name %>-service.js --debug-brk",
    "test": "mocha specs --reporter spec",
    "docker": "node scripts/docker.js"
  },
  "engines": {
    "node": ">=6.2.1"
  },
  "dependencies": {
    <%_deps.forEach((module,
    i)=>{
      _%>"<%= module %>": "<%= versions[module] %>"<%_if(i<deps.length-1){
        %>,
        <%_
      }%><%_
    })_%>
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.16.0",
    "mocha": "3.2.0",
    "superagent": "3.5.2"
  }
}