{
  "name": "<%= name %>-service",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node <%= name %>-service.js",
    "debug": "node-debug <%= name %>-service.js --debug-brk",
    "test": "mocha specs --reporter spec",
    "docker": "node scripts/docker.js"
  },
  "engines": {
    "node": ">=6.2.1"
  },
  "dependencies": {
    <%_ deps.forEach((module, i) => { _%>
    "<%= module %>": "<%= versions[module] %>"<%_ if (i<deps.length-1) {%>,<%_}%>
    <%_ }) _%>
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.7.1",
    "mocha": "3.1.1",
    "superagent": "2.3.0"
  }
}
