{
  "name": "<%= name %>-service",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node <%= name %>-service.js",
    "debug": "node-debug <%= name %>-service.js",
    "test": "mocha specs --reporter spec",
    "docker": "node scripts/docker.js"
  },
  "engines": {
    "node": ">=6.2.1"
  },
  "dependencies": {
    <%_ deps.forEach(module => { _%>
    "<%= module %>": <%= versions[module] %>
    <%_ }) _%>
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.7.1",
    "jscs": "3.0.7",
    "jscs-jsdoc": "2.0.0",
    "mocha": "3.1.1",
    "superagent": "2.3.0"
  }
}
